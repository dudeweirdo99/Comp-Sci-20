	public static void showGrades() {
		
		for(int j = 1; j < 13; j++) {
			System.out.print("Student "+j+": ");
			for(int i = 0; i < 5; i++) {
				System.out.print(grades[j-1][i]+" ");
			}
			System.out.println("");
		}
	}
	
	public static void studentAvg() {
		
		for(int j = 1; j < 13; j++) {
			System.out.print("Student "+j+":");
			for(int i = 0; i < 5; i++) {
				st1 += grades[j-1][i];
			}
			System.out.print(st1/5)
		}
	}
	
	public static void testAvg() {
		
	}
