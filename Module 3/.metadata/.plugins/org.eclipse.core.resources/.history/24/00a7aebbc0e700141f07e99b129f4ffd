package Exercises;
import java.util.*;

public class Exercise12 {

	static Scanner input = new Scanner(System.in);
	
	static int grades[][] = new int[12][5];
	static int st1, st2, st3, st4, st5;
	
	public static void main(String[] args) {
		
		
		showGrades();
	}
	
	public static void getGrades() {
		
		System.out.println("Enter grades for student 1: ");
		grades[0][0] = input.nextInt();
		grades[0][1] = input.nextInt();
		grades[0][2] = input.nextInt();
		grades[0][3] = input.nextInt();
		grades[0][4] = input.nextInt();
		
