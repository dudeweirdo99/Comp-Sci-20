		grades[5][2] = input.nextInt();
		grades[5][3] = input.nextInt();
		grades[5][4] = input.nextInt();
		
		System.out.println("Enter grades for student 7: ");
		grades[6][0] = input.nextInt();
		grades[6][1] = input.nextInt();
		grades[6][2] = input.nextInt();
		grades[6][3] = input.nextInt();
